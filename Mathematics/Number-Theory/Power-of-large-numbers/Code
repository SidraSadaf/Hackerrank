#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#define mod 1000000007
using namespace std;

void power(string a,string b){
    long int lena=a.size();
    long int lenb=b.size();
    long long int x=0,y=0;
    for(long int i=0;i<lena;++i){
        x=(x*10+(int)(a[i]-'0'))%mod;
    }
    for(long int i=0;i<lenb;++i){
        y=(y*10+(int)(b[i]-'0'))%(mod-1);
    }
    long long int p=1;
    while(y>0){
        if(y%2==1){
            p=((p%mod)*(x%mod))%mod;
        }
        x=((x%mod)*(x%mod))%mod;
        y/=2;
    }
    cout<<p%mod<<endl;
}

int main() {
    int t;
    cin>>t;
    for(int i=0;i<t;++i){
        string a,b;
        cin>>a>>b;
        power(a,b);
    }
    return 0;
}
